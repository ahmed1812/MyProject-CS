@model List<ProductCategory>

<h2>1- Retrieve the name and price of all products in the "Computers" category:</h2>

<table class="table">
    <thead>
        <tr>
            <th>Product Name</th>
            <th>Product Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var pc in Model)
        {
            <tr>
                <td>@pc.Name</td>
                <td>@pc.Price</td>
            </tr>
        }
    </tbody>
</table>